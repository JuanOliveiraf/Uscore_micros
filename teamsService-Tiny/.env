##############################################################################
# Environment variables for teamsService-Tiny                                 #
# Plain KEY=VALUE pairs only. Remove any Python code (dotenv can't parse it). #
##############################################################################

# CORS origins (comma-separated)
CORS_ORIGINS=http://localhost:5173,http://localhost:3000

# TinyDB file path
DB_PATH=data/teams.json

# Service identification
SERVICE_NAME=teamsService - TinyDB

# API prefix
API_PREFIX=/api/v1

# External integrations
COMPETITIONS_BASE_URL=http://localhost:8002

# Port the service listens on
PORT=8001

# Request timeout (seconds) for outbound HTTP calls
REQUEST_TIMEOUT=3.0
